@inproceedings{jorge_c._lucero_vocal_2014,
	address = {Salt Lake City},
	title = {A vocal fold model for disordered voice synthesis},
	doi = {10.13140/2.1.3278.2726},
	abstract = {We present our progress in the development of a vocal fold model capable of mimicking the vocal quality of speakers that suffer from a voice or speech pathology or dysfunction. Although several models have already been proposed to characterize complex oscillation patterns and vocal instabilities observed in patients, their application to actual vocal quality synthesis has been limited due to non-smoothness and even discontinuities of the associated equations. In our work, we propose a smooth version of a previous lumped mucosal wave representation of the vocal folds. The model is coupled to a concatenated-tube representation of the trachea and vocal tract, and allows for the addition of controlled levels of muscle jitter, neurological and physiological tremor, pulsatile and aspiration noise due to airflow turbulence, and tension imbalance between left and right vocal folds. Potential applications include its use as a computational tool for the education of voice clinicians, speech and language pathologist and physicians. [Work supported by CNPq (Brazil) and FRS/FNRS (Belgium)].},
	publisher = {Jorge C. Lucero},
	author = {Jorge C. Lucero and Jean Schoentgen and Mara Behlau and Glaucya Madazio},
	month = sep,
	year = {2014},
	keywords = {tcc}
}

@article{dennis_h._klatt_software_1980,
	title = {Software for a cascade/parallel formant synthesizer},
	volume = {67},
	url = {http://www.fon.hum.uva.nl/david/ma_ssp/doc/Klatt-1980-JAS000971.pdf},
	abstract = {A software formant synthesizer isdescribed that can generate synthetic speech using a laboratory digital
computer. A flexible synthesizer configuration permits the synthesis of sohorants by either a cascade or
parallel connection of digital resonators, but frication spectra must be synthesized by a set of resonators
connected in parallel. A control program lets the user specify variable control parameter data, such as
formant frequencies as a function of time, as a sequence of (time, value) points. The synthesizer design
is described and motivated in Sees. I-lII, and FORTRAn listings for the synthesizer and control
program are provided in an appendix. Computer requirements and necessary support software are
described in Sec. IV. Strategies for the imitation of any speech utterance are described in Sec. V, and
suggested values of control parameters for the synthesis of many English sounds are presented in tabular
form.},
	language = {English},
	number = {3},
	journal = {The Journal of the Acoustical Society of America},
	author = {Dennis H. Klatt},
	month = jun,
	year = {1980},
	pages = {971--995}
}